{
  "name": "cra-redux-observable-ts-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/start.js --env=dev",
    "build": "node scripts/build.js",
    "analyze": "node scripts/build.js --report=true",
    "test": "node scripts/test.js",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src"
  },
  "dependencies": {
    "@loadable/component": "^5.12.0",
